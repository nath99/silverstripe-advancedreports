{
	"name": "silverstripe-australia/advancedreports",
	"description": "A reporting module that provides an extra level of configuration and functionality over the base SilverStripe structures. Generates static output content in HTML, CSV and PDF formats.",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "reports"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Marcus Nyeholt",
			"email": "marcus@silverstripe.com.au"
		}
	],
	"require": {
		"silverstripe/cms": "^4",
		"silverstripe/multivaluefield": "*",
		"silverstripe/vendor-plugin": "^1.6",
		"silverstripe/queuedjobs": "^4.9",
		"symbiote/silverstripe-multivaluefield": "^4.0",
		"symbiote/silverstripe-gridfieldextensions": "^3.6"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	},
    "autoload": {
        "psr-4": {
            "SilverstripeAustralia\\AdvancedReports\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "css",
            "images",
            "javascript"
        ]
    }
}
